{
    "jobDefinitionName": "transcode",
    "type": "container",
    "retryStrategy": {
        "attempts": 3
    },
    "containerProperties": {
        "image": "quay.io/mojodna/mapzen-dynamic-tiler-batch",
        "vcpus": 1,
        "memory": 1500,
        "command": [
            "process.sh",
            "Ref::input",
            "Ref::output"
        ],
        "jobRoleArn": "arn:aws:iam::{{AWS_ACCOUNT_ID}}:role/transcoder",
        "privileged": true
    }
}
